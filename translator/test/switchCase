public class Main
{
    public static void main(String[] args) {
		int i=3;
		switch (i){
		    case 0:
		        i+=1;
		        break;
		    case 2:
		        i-=1;
		        break;
		    case 3:
		        i-=2;
		        break;
            default:
                i = 1;
		}
		System.out.println(i);
    }
}