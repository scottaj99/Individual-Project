package hello;
import math.numpy.py;
import hello.*;
class HelloWorld {
    public static void main(String[] args, int i) {
		while(i<10)
		{
			if (i<5){
				//hello
				/* this is an example
				of
				a
				multi
				line
				comment */
				System.out.println(i);
				i+=2;
			}
			else {
				System.out.println(i);
				i-=1;
			} 
		}
    }
}